\RequirePackage[utf8]{inputenc}

\newif\ifunicode

\ifxetex\unicodetrue\fi
\ifluatex\unicodetrue\fi

\ifunicode
  \newcommand \DeclareUnicodeCharacter [2] {%
    \begingroup\lccode`|=\string"#1\relax
    \lowercase{\endgroup\newunicodechar{|}}{#2} %
}
\else 
\fi

\def \uni + 0x#1 | #2 | #3 +{\relax
  \def\myhex{#1}\relax
  \def\mymacro{#2}\relax
  \def\myuni{#3}\relax
  \@ifnextchar*
  {\DeclareUnicodeCharacter{\myhex}{\ensuremath{\mymacro}}}\relax
  {\DeclareUnicodeCharacter{\myhex}{\mymacro}}\relax
}

